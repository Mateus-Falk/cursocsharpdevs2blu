@model List<Devs2blu.ProjetosAula.Ativiade3MVC.Models.DTOAPI.DigimonDTO>

<table class="table table-dark table-striped">
		<thead>
			<tr>
				<th scope="col">Name</th>
				<th scope="col">Picture</th>
				<th scope="col">Level</th>
			</tr>
		</thead>
		<tbody>
			@foreach(var digimon in Model)
			{
				<tr>
					<td>@digimon.name</td>
					<td><img class="img-fluid" src="@digimon.img" alt="@digimon.name"></td>
					<td>@digimon.level</td>
				</tr>
			}
		</tbody>
	</table>
